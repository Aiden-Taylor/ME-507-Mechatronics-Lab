// motor.h

#ifndef INC_MOTOR_H_
#define INC_MOTOR_H_

#include <stdio.h>
#include <stdint.h>

// Motor object data structure
struct {
    int32_t  duty;
    uint32_t channel;
    uint32_t ccr1;
    uint32_t ccr2;
} typedef motor_t;

// Prototype for motor object "method"
void set_duty(motor_t* p_mot, int32_t duty);

#endif /* INC_MOTOR_H_ */
